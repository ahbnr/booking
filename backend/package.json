    "@react-pdf/renderer": "^2.0.19",
    "@sendgrid/mail": "^7.4.6",
    "autobind-decorator": "^2.4.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "common": "1.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fp-ts": "^2.8.3",
    "full-icu": "^1.3.4",
    "helmet": "^4.6.0",
    "humanize-duration": "^3.27.0",
    "i18next": "^20.3.5",
    "io-ts": "^2.2.11",
    "jsonwebtoken": "^8.5.1",
    "lazy-get-decorator": "^2.2.0",
    "luxon": "^1.25.0",
    "mariadb": "^2.4.2",
    "moment": "^2.27.0",
    "mysql2": "^2.1.0",
    "node-schedule": "^2.0.0",
    "nodemailer": "^6.6.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "secure-random-password": "^0.2.1",
    "sequelize": "5.22.2",
