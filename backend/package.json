{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index",
  "scripts": {
    "clear-memory": "rimraf memory",
    "run-index": "DEV_MODE=1 NODE_ICU_DATA=\"$(pwd)/node_modules/full-icu\" ts-node src/index.ts",
    "start-unwatched": "run-s lint run-index",
    "start": "nodemon --watch src --ext \"js,mjs,jsx,ts,tsx,json\" --exec yarn start-unwatched",
    "clear-memory-start": "run-s clear-memory start",
    "start-prod": "NODE_ICU_DATA=\"$(pwd)/node_modules/full-icu\" node dist/index.js",
    "compile-ts": "tsc --build tsconfig.json",
    "build": "run-s lint compile-ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "run-vrest-now": "vrest-ng-cli run --projectdir=./testing/vREST_NG --testsuitenames=\"Shallow API Tests\" --logger=xunit",
    "wait-for-api": "wait-on tcp:3000",
    "run-vrest-when-ready": "run-s wait-for-api run-vrest-now",
    "test-old": "concurrently -k -s first yarn:clear-memory-start yarn:run-vrest-when-ready",
    "test": "echo \"Currently no isolated tests for backend. Use end-to-end tests in frontend.\""
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0-only",
