{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/core": "1.x",
    "@date-io/luxon": "1.x",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "autobind-decorator": "^2.4.0",
    "cldr-data": "^36.0.0",
    "clsx": "^1.1.1",
    "color-hash": "^2.0.1",
    "common": "1.0.0",
    "detect-browser": "^5.2.0",
    "file-saver": "^2.0.5",
    "i18next": "^20.2.4",
    "io-ts": "^2.2.11",
    "lodash": "^4.17.19",
    "luxon": "^1.25.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^7.12.2",
    "react-i18next": "^11.8.15",
    "react-scripts": "3.4.1",
    "react-virtualized-auto-sizer": "^1.0.5",
    "react-window": "^1.8.6",
    "react-window-infinite-loader": "^1.0.7",
    "ts-adt": "^1.0.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "typecheck": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "start": "PORT=8000 HTTPS=true SSL_CRT_FILE=../dev-certificate/generated/server.crt SSL_KEY_FILE=../dev-certificate/generated/server.key REACT_APP_DEV_MODE=1 react-scripts start",
    "start-no-browser": "BROWSER=none run-s start",
    "start-prod": "node static-server/static-server.js",
    "react-scripts-build": "react-scripts build",
    "build": "GENERATE_SOURCEMAP=true run-s react-scripts-build",
    "heroku-postbuild": "run-s build",
    "eject": "react-scripts eject",
    "test-start-backend": "yarn workspace backend clear-memory-start",
    "test-wait-for-backend": "yarn workspace backend wait-for-api",
    "test-wait-for-frontend": "wait-on tcp:8000",
    "test-start-cypress": "cypress run",
    "test-run-cypress-when-ready": "run-s test-wait-for-backend test-wait-for-frontend test-start-cypress",
    "test-old": "concurrently -k -s first yarn:test-start-backend yarn:start-no-browser yarn:test-run-cypress-when-ready",
    "test": "echo \"Currently no frontend tests. Create some using cypress.\"",
    "source-map-explorer": "source-map-explorer 'build/static/js/*.js'",
    "analyze": "run-s react-scripts-build source-map-explorer"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/color-hash": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/file-saver": "^2.0.3",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.159",
    "@types/luxon": "^1.25.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-i18next": "^8.1.0",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/react-window-infinite-loader": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "compression": "^1.7.4",
    "concurrently": "^6.1.0",
    "cypress": "^7.2.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-unused-imports": "^1.1.3",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "http-proxy-middleware": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "source-map-explorer": "^2.5.2",
    "wait-on": "^5.3.0"
  }
}
